<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ACheckBox">
        <attr name="image_unselected" format="reference" />
        <attr name="image_selected" format="reference" />
        <attr name="select_state" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RoundImageView">
        <attr name="image_radius" format="dimension" />
        <attr name="image_type">
            <enum name="circle" value="0" />
            <enum name="round" value="1" />
        </attr>
    </declare-styleable>
</resources>
